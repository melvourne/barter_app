<div class="container">
    <%= simple_form_for @post do |f| %>
      <%= f.input :photo, multiple: true %>
      <%= f.input :title %>
      <%= f.input :description, placeholder: 'Enter a brief description and what are you seeking for barter' %>
      <%= f.input :price %>
      <%= f.input :suburb %>
      <%= f.button :submit %>
    <% end %>
</div>


